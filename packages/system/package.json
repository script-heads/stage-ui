{
  "name": "@stage-ui/system",
  "description": "Flexible UI components library declared by your own Design System.",
  "version": "0.7.3",
  "license": "MIT",
  "homepage": "https://stageui.shds.io/",
  "bugs": "https://github.com/script-heads/stage-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/script-heads/stage-ui.git",
    "directory": "packages/system"
  },
  "keywords": [
    "stageui",
    "uikit",
    "react",
    "@stage-ui",
    "ui",
    "components",
    "stage",
    "library",
    "framework"
  ],
  "scripts": {
    "make": "node ../../scripts/make system",
    "build": "yarn build:types && yarn build:js",
    "build:js": "babel ./ --out-dir ../../build/system --extensions \".ts,.tsx\" --ignore \"node_modules/\"",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "gen:declarations": "tsc --project tsconfig.docs.json",
    "lint": "eslint --config .eslintrc.json --fix '**/*.+(js|jsx|ts|tsx)'"
  },
  "dependencies": {
    "@types/color": "^3.0.2",
    "color": "^3.1.3",
    "@emotion/cache": "^11.7.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "@emotion/react": ">=11"
  }
}