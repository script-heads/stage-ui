{
  "name": "stage-ui",
  "author": "Project one",
  "version": "0.2.0-beta",
  "license": "MIT",
  "private": true,
  "workspaces": [
      "packages/*"
  ],
  "scripts": {
    "build": "yarn system:build && yarn icons:build && yarn core:build",
    "pub": "yarn core:pub && yarn icons:pub && yarn system:pub",
    "core:build": "cd ./packages/core && yarn build",
    "core:pub": "cd ./build/core && npm publish --registry=https://registry.npmjs.org",
    "icons:build": "cd ./packages/icons && yarn build",
    "icons:pub": "cd ./build/icons && npm publish --registry=https://registry.npmjs.org",
    "system:build": "cd ./packages/system && yarn build",
    "system:pub": "cd ./build/system && npm publish --registry=https://registry.npmjs.org",
    "docs:start": "yarn workspace @stage-ui/docs start",
    "docs:build": "cd ./packages/docs && yarn build",
    "docs:serve": "cd ./build/docs && pm2 start --name StageUI server.js",
    "docs:test": "yarn workspace @stage-ui/docs test",
    "docs:test:approve": "yarn workspace @stage-ui/docs approve",
    "sandbox:start": "yarn workspace @stage-ui/sandbox start",
    "sandbox:analyze": "yarn workspace @stage-ui/sandbox analyze",
    "gen": "yarn gen:createTypes && yarn gen:generateTypes && yarn gen:fixTypes",
    "gen:createTypes": "cd ./packages/docs && typedoc",
    "gen:generateTypes": "cd ./packages/docs && node ./scripts/generateTypes.js",
    "gen:fixTypes": "cd ./packages/docs && node ./scripts/fixTypes.js",
    "gen:icons": "cd ./packages/icons && yarn gen"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/react": "11.4.1",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/webpack-env": "1.14.1",
    "css-loader": "5.0.1",
    "esbuild-loader": "2.13.0",
    "esbuild": "0.13.3",
    "eslint": "7.27.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "express": "4.17.1",
    "file-loader": "6.2.0",
    "source-map-loader": "2.0.0",
    "prettier": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "transform-imports": "2.1.1",
    "typescript": "4.2.4",
    "webpack": "5.38.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-filter-warnings-plugin": "1.2.1",
    "copy-webpack-plugin": "9.0.0",
    "form-data": "4.0.0",
    "body-parser": "1.19.0",
    "express-form-data": "2.0.17",
    "ws": "8.2.2",
    "axios": "0.22.0",
    "element-resize-event": "3.0.6"
  }
}
